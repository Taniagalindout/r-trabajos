func1 <- function (x, arithmetic=TRUE){
if(is.character(x) | is.logical(x)){
print("El argumento no es numerico")
}else{
if(arithmetic==TRUE){
sum(x)/length(x)
}else{
prod(x)^(1/length(x))
}
}
return("Hola")
}
resultado <- func1(c(1,2,3), TRUE)
